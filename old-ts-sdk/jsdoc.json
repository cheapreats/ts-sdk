{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ".",
    "includePattern": "(app/)",
    "excludePattern": "(node_modules/|docs|build|lib|test)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}